<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hrayr.polytech">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Dasacucak"
            android:label="@string/title_activity_dasacucak"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Exams"
            android:label="@string/title_activity_exams"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ExamListActivity"
            android:label="@string/title_exam_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ExamDetailActivity"
            android:label="@string/title_exam_detail"
            android:parentActivityName=".ExamListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.hrayr.polytech.ExamListActivity" />
        </activity>
        <activity android:name=".StudentIdinput" />
        <activity
            android:name=".MyInfo"
            android:label="@string/title_activity_my_info"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".workListActivity"
            android:label="@string/title_work_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".workDetailActivity"
            android:label="@string/title_work_detail"
            android:parentActivityName=".workListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.hrayr.polytech.workListActivity" />
        </activity>
        <activity
            android:name=".StugarqListActivity"
            android:label="@string/title_stugarq_list"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".StugarqDetailActivity"
            android:label="@string/title_stugarq_detail"
            android:parentActivityName=".StugarqListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.hrayr.polytech.StugarqListActivity" />
        </activity>
    </application>

</manifest>